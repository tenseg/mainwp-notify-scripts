<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Disabled</key>
		<true />
		<key>EnvironmentVariables</key>
		<dict>
			<key>PATH</key>
			<string>
				/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/MacGPG2/bin:/Library/Apple/usr/bin:/opt/local/sbin:/opt/homebrew/bin:/opt/homebrew/sbin</string>
		</dict>
		<key>Label</key>
		<string>net.tenseg.MainWP.updates</string>
		<key>ProgramArguments</key>
		<array>
			<string>{PATH_TO_updates.php}</string>
		</array>
		<key>RunAtLoad</key>
		<false />
		<key>StartCalendarInterval</key>
		<array>
			<dict>
				<key>Hour</key>
				<integer>15</integer>
				<key>Minute</key>
				<integer>30</integer>
			</dict>
		</array>
	</dict>
</plist>